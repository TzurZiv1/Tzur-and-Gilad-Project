<Window x:Class="PLWPF.RemoveNannyWin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PLWPF"
        mc:Ignorable="d"
        Title="RemoveNannyWin" Height="300" Width="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>

        <Label Grid.Row="0" Content="Remove Nanny" FontSize="25" Margin="0" HorizontalContentAlignment="Center"/>
        <Button x:Name="RemoveNanny" Grid.Row="2" Content="Add" Width="150" Height="30" Margin="0,5" Click="RemoveNanny_Click"/>
        
        
        
    </Grid>
</Window>
